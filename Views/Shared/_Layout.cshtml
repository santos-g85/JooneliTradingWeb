<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] Jooneli Trading - Complete inventory management solution for modern businesses</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/JooneliTrading.styles.css" asp-append-version="true" />

    <link rel="icon" type="image/x-icon" href="~/images/logo.jpg" />

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.0/font/bootstrap-icons.css">


    <!-- SEO Meta Tags -->
    <meta name="keywords" content="Jooneli Trading" />
    <meta name="robots" content="index, follow" />
    <meta name="title" content="Jooneli Trading"/>
    <meta name="description" content="Inventory Management System." />

    <!-- Open Graph Meta -->
    <meta property="og:title" content="Jooneli Trading" />
    <meta property="og:description" content="Jooneli Trading" />
    <meta property="og:type" content="website" />
    <meta property="og:url" content="http://jooneli.com/" />
    <meta property="og:image" content="http://jooneli.com/" />
    <meta property="og:image:alt" content="Jooneli Trading" />

    <!-- Twitter Meta -->
    <meta name="twitter:card" content="summary_large_image" />
    <meta name="twitter:title" content="Jooneli Trading" />
    <meta name="twitter:description" content="Jooneli Trading" />
    <meta name="twitter:image" content="http://jooneli.com/images/logo.png" />

</head>
<body>

 
    <header>
        <!-- Top navbar (visible on large screens only) -->
        <nav class="navbar navbar-expand-sm navbar-light bg-white border-bottom d-none d-md-flex">
            <div class="container-fluid justify-content-between">
                <a class="navbar-brand fw-bold text-dark" href="#">
                    JooneliTrading
                </a>
                <ul class="navbar-nav mb-2 mb-sm-0">
                    <li class="nav-item mx-2"><a class="nav-link text-dark" href="#">Fe</a></li>
                    <li class="nav-item mx-2"><a class="nav-link text-dark" href="#">Pri</a></li>
                    <li class="nav-item mx-2"><a class="nav-link text-dark" href="https://www.jooneli.com/About" target="_blank">Abo</a></li>
                    <li class="nav-item mx-2"><a class="nav-link text-dark" href="#">Cont</a></li>
                </ul>
            </div>
        </nav>

        <!-- Bottom navbar (collapsible on small devices) -->
        <nav class="navbar navbar-expand-md navbar-light bg-white ">
            <div class="container-fluid">
                <a class="navbar-brand fw-bold text-dark navbar-brand-centered" href="#">
                    JooneliTrading
                </a>

                <!-- Toggler for mobile -->
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse"
                        data-bs-target="#navbarMenu" aria-controls="navbarMenu"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>

                <!-- Collapsible menu -->
                <div class="collapse navbar-collapse" id="navbarMenu">
                    <!-- Menu items shown only on small devices -->
                    <ul class="navbar-nav me-auto mb-2 mb-md-0 d-md-none">
                        <li class="nav-item mx-2"><a class="nav-link text-dark" href="#">Fe</a></li>
                        <li class="nav-item mx-2"><a class="nav-link text-dark" href="#">Pri</a></li>
                        <li class="nav-item mx-2"><a class="nav-link text-dark" href="https://www.jooneli.com/About" target="_blank">Abo</a></li>
                        <li class="nav-item mx-2"><a class="nav-link text-dark" href="#">Cont</a></li>
                    </ul>

                    <!-- Regular lower menu (always visible) -->
                    <ul class="navbar-nav me-auto mb-2 mb-md-0">
                        <li class="nav-item mx-2"><a class="nav-link text-dark" href="#">Features</a></li>
                        <li class="nav-item mx-2"><a class="nav-link text-dark" href="#">Pricing</a></li>
                        <li class="nav-item mx-2"><a class="nav-link text-dark" href="https://www.jooneli.com/About" target="_blank">About</a></li>
                        <li class="nav-item mx-2"><a class="nav-link text-dark" href="#">Contact</a></li>
                    </ul>

                    <!-- Button (hidden on mobile) -->
                    <button class="btn btn-primary ">Start Free Trial</button>
                </div>
            </div>
        </nav>
    </header>


    
        <main role="main" >
            @RenderBody()
        </main>
    

    @await Html.PartialAsync("_Footer")

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
